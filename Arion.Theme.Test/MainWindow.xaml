                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>

                <Label
                    Content="Shadow"
                    FontSize="32"
                    Grid.Column="0"
                    Grid.ColumnSpan="5"
                    Grid.Row="0"
                    HorizontalAlignment="Left"
                    Margin="10"
                    Padding="0"
                    Style="{StaticResource Lbl}"
                    VerticalAlignment="Top"
                    Width="Auto" />

                <Button
                    Effect="{StaticResource El1}"
                    Grid.Column="0"
                    Grid.Row="1"
                    Margin="20"
                    Style="{StaticResource OutlineLg}" />
                <Button
                    Effect="{StaticResource El2}"
                    Grid.Column="1"
                    Grid.Row="1"
                    Margin="20"
                    Style="{StaticResource OutlineLg}" />
                <Button
                    Effect="{StaticResource El3}"
                    Grid.Column="2"
                    Grid.Row="1"
                    Margin="20"
                    Style="{StaticResource OutlineLg}" />
                <Button
                    Effect="{StaticResource El4}"
                    Grid.Column="3"
                    Grid.Row="1"
                    Margin="20"
                    Style="{StaticResource OutlineLg}" />
                <Button
                    Effect="{StaticResource El5}"
                    Grid.Column="4"
                    Grid.Row="1"
                    Margin="20"
                    Style="{StaticResource OutlineLg}" />
                <Button
                    Effect="{StaticResource El6}"
                    Grid.Column="5"
                    Grid.Row="1"
                    Margin="20"
                    Style="{StaticResource OutlineLg}" />
            </Grid>
        </TabItem>
        <TabItem Header="Sliders">
            <!--  SLIDERS  -->
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Label
                    Content="Slider"
                    FontSize="32"
                    Grid.Column="0"
                    Grid.Row="0"
                    HorizontalAlignment="Left"
                    Margin="10"
                    Padding="0"
                    Style="{StaticResource Lbl}"
                    VerticalAlignment="Top"
                    Width="Auto" />
                <Slider
                    Grid.Column="1"
                    Grid.Row="1"
                    Margin="5"
                    Width="200" />
                <Slider
                    Grid.Column="1"
                    Grid.Row="2"
                    Margin="5"
                    Style="{StaticResource SliderBig}"
                    Width="200" />
                <Slider
                    Grid.Column="1"
                    Grid.Row="3"
                    IsSelectionRangeEnabled="True"
                    Margin="5"
                    Width="200" />

            </Grid>
        </TabItem>
        <TabItem Header="Stepper">
            <!--  STEPPERS  -->
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Label
                    Content="BUILD"
                    FontSize="144"
                    FontWeight="Black"
                    Foreground="{StaticResource Lg3}"
                    Grid.Column="0"
                    Grid.ColumnSpan="20"
                    Grid.Row="0"
                    Grid.RowSpan="20"
                    HorizontalAlignment="Center"
                    Margin="10"
                    Opacity=".7"
                    Padding="0"
                    Style="{StaticResource Lbl}"
                    VerticalAlignment="Center"
                    Width="Auto" />
            </Grid>
        </TabItem>
        <TabItem Header="Switchers">
            <!--  SWITCHERS  -->
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Label
                    Content="Switcher"
                    Grid.Column="0"
                    Grid.Row="0"
                    Margin="10"
                    Padding="0"
                    Style="{StaticResource Lbl}"
                    VerticalAlignment="Top"
                    Width="Auto" />

                <Label
                    Content="Filled"
                    Grid.Column="1"
                    Grid.Row="1"
                    HorizontalAlignment="Left"
                    Style="{StaticResource Lbl}" />

                <StackPanel
                    Grid.Column="1"
                    Grid.Row="2"
                    Orientation="Horizontal">
                    <RadioButton
                        Content="First"
                        Margin="5"
                        Style="{StaticResource SwitcherFirst}" />
                    <RadioButton
                        Content="Middle"
                        Margin="5"
                        Style="{StaticResource SwitcherMiddle}" />
                    <RadioButton
                        Content="Middle"
                        IsEnabled="False"
                        Margin="5"
                        Style="{StaticResource SwitcherMiddle}" />
                    <RadioButton
                        Content="Last"
                        Margin="5"
                        Style="{StaticResource SwitcherLast}" />
                </StackPanel>
            </Grid>
        </TabItem>
        <TabItem Header="Tabs">
            <!--  TABS  -->
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <Label
                    Content="Tabs"
                    FontSize="32"
                    Grid.Column="0"
                    Grid.Row="0"
                    HorizontalAlignment="Left"
                    Margin="10"
                    Padding="0"
                    Style="{StaticResource Lbl}"
                    VerticalAlignment="Top"
                    Width="Auto" />

                <DockPanel
                    Grid.Column="0"
                    Grid.ColumnSpan="2"
                    Grid.Row="1">
                    <TabControl DockPanel.Dock="Left" TabStripPlacement="Left">
                        <TabItem Header="Head 1 Content" />
                        <TabItem Header="Head 2" />
                        <TabItem Header="Head 3" />
                        <TabItem Header="Head 4" />
                    </TabControl>
                    <TabControl DockPanel.Dock="Top" TabStripPlacement="Top">
                        <TabItem Header="Head 1" />
                        <TabItem Header="Head 2" />
                        <TabItem Header="Head 3" />
                        <TabItem Header="Head 4" />
                    </TabControl>
                    <TabControl DockPanel.Dock="Right" TabStripPlacement="Right">
                        <TabItem Header="Head 1" />
                        <TabItem Header="Head 2" />
                        <TabItem Header="Head 3" />
                        <TabItem Header="Head 4" />
                    </TabControl>
                    <TabControl DockPanel.Dock="Bottom" TabStripPlacement="Bottom">
                        <TabItem Header="Head 1" />
                        <TabItem Header="Head 2" />
                        <TabItem Header="Head 3" />
                        <TabItem Header="Head 4" />
                    </TabControl>
                </DockPanel>
            </Grid>
        </TabItem>
        <TabItem Header="Tables">
            <!--  TABLES  -->
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>

                <DataGrid
                    CellStyle="{DynamicResource DataGridCellStyle1}"
                    ColumnHeaderStyle="{DynamicResource DataGridColumnHeaderStyle1}"
                    Grid.Column="0"
                    Grid.Row="1"
                    IsSelected="True"
                    Style="{DynamicResource DataGridStyle2}">
                    <DataGrid.Columns>
                        <DataGridTextColumn Header="Test column" Width="*" />
                        <DataGridCheckBoxColumn Header="Checkbox" Width="*" />
                        <DataGridComboBoxColumn Header="ComboBox" Width="*" />
                        <DataGridHyperlinkColumn Header="Hyperlink" Width="*" />
                        <DataGridTemplateColumn Header="Template" Width="*">
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <Button Content="Press me" />
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                        </DataGridTemplateColumn>
                    </DataGrid.Columns>
                </DataGrid>
            </Grid>
        </TabItem>
        <!--  TEXT BOXES  -->
        <TabItem Header="Text boxes">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Label
                    Content="TextBox"
                    FontSize="32"
                    Grid.Column="0"
                    Grid.Row="0"
                    HorizontalAlignment="Left"
                    Margin="10"
                    Padding="0"
                    Style="{StaticResource Lbl}"
                    VerticalAlignment="Top"
                    Width="Auto" />

                <TextBox
                    FontWeight="Light"
                    Grid.Column="0"
                    Grid.Row="1"
                    Margin="5"
                    Width="200"
                    attachedProperties:TextBox.Placeholder="Placeholder" />
                <TextBox
                    Grid.Column="0"
                    Grid.Row="2"
                    Margin="5"
                    Width="200" />
                <TextBox
                    Grid.Column="0"
                    Grid.Row="3"
                    IsEnabled="False"
                    Margin="5"
                    Text="Disabled"
                    Width="200" />
                <TextBox
                    Grid.Column="0"
                    Grid.Row="4"
                    IsReadOnly="True"
                    Margin="5"
                    Width="200" />
                <TextBox
                    Grid.Column="0"
                    Grid.Row="5"
                    IsReadOnly="True"
                    Margin="5"
                    Text="Text"
                    Width="200" />
            </Grid>
        </TabItem>

        <!--  TIME INPUT  -->
        <TabItem Header="Time inputs">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Label
                    Content="Time input"
                    FontSize="32"
                    Grid.Column="0"
                    Grid.Row="0"
                    HorizontalAlignment="Left"
                    Margin="10"
                    Padding="0"
                    Style="{StaticResource Lbl}"
                    VerticalAlignment="Top"
                    Width="Auto" />
                <controls:TimeInput
                    FontSize="58"
                    Grid.Column="1"
                    Grid.Row="1"
                    Height="130"
                    Margin="0"
                    Name="TiTime"
                    Width="480" />
            </Grid>
        </TabItem>
        <TabItem Header="Toggle buttons">
            <!--  TOGGLE BUTTONS  -->
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>

                <Label
                    Content="ToggleButton"
                    FontSize="32"
                    Grid.Column="0"
                    Grid.Row="0"
                    HorizontalAlignment="Left"
                    Margin="10"
                    Padding="0"
                    Style="{StaticResource Lbl}"
                    VerticalAlignment="Top"
                    Width="Auto" />

                <StackPanel
                    Grid.Column="0"
                    Grid.Row="1"
                    Orientation="Horizontal">
                    <ToggleButton
                        Content="Toggle button"
                        Margin="5"
                        Name="Tg1" />
                    <ToggleButton
                        Content="Toggle button"
                        IsChecked="{Binding ElementName=Tg1, Path=IsChecked}"
                        IsEnabled="False"
                        Margin="5" />
                </StackPanel>

                <StackPanel
                    Grid.Column="0"
                    Grid.Row="2"
                    Orientation="Horizontal">
                    <ToggleButton
                        Content="Toggle button"
                        IsChecked="True"
                        Margin="5"
                        Name="Tg2" />
                    <ToggleButton
                        Content="Toggle button"
                        IsChecked="{Binding ElementName=Tg2, Path=IsChecked}"
                        IsEnabled="False"
                        Margin="5" />
                </StackPanel>

                <StackPanel
                    Grid.Column="0"
                    Grid.Row="3"
                    Orientation="Horizontal">
                    <ToggleButton
                        Content="Toggle button"
                        IsChecked="{x:Null}"
                        IsThreeState="True"
                        Margin="5"
                        Name="Tg3" />
                    <ToggleButton
                        Content="Toggle button"
                        IsChecked="{Binding ElementName=Tg3, Path=IsChecked}"
                        IsEnabled="False"
                        IsThreeState="True"
                        Margin="5" />
                </StackPanel>

                <StackPanel
                    Grid.Column="0"
                    Grid.Row="4"
                    Orientation="Horizontal">
                    <ToggleButton
                        Content="Toggle button"
                        Margin="5"
                        Style="{StaticResource TgOutlineLg}" />
                    <ToggleButton
                        Content="Toggle button"
                        IsChecked="True"
                        Margin="5"
                        Style="{StaticResource TgOutlineLg}" />
                </StackPanel>

                <StackPanel
                    Grid.Column="0"
                    Grid.Row="5"
                    Margin="0,5"
                    Orientation="Horizontal">
                    <ToggleButton Content="First" Style="{StaticResource TgOutlineFirstLg}" />
                    <ToggleButton Content="Middle" Style="{StaticResource TgOutlineMiddleLg}" />
                    <ToggleButton Content="last" Style="{StaticResource TgOutlineLastLg}" />
                </StackPanel>

                <StackPanel
                    Grid.Column="0"
                    Grid.Row="6"
                    Orientation="Horizontal">
                    <ToggleButton
                        Margin="5"
                        Name="TgListControl"
                        Style="{StaticResource ListControlSelectMode}" />
                    <ToggleButton
                        IsChecked="{Binding ElementName=TgListControl, Path=IsChecked}"
                        IsEnabled="False"
                        Margin="5"
                        Style="{StaticResource ListControlSelectMode}" />
                </StackPanel>
            </Grid>
        </TabItem>
        <TabItem Header="Tooltips">
            <!--  TOOLTIPS  -->
            <Button
                Content="asd"
                Height="50"
                ToolTip="Hello, i'm tooltip"
                Width="200"
                d:IsLocked="True" />
        </TabItem>
    </TabControl>
</Window>