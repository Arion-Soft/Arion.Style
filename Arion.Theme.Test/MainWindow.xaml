                    Margin="10"
                    Padding="0"
                    Style="{StaticResource Lbl}"
                    VerticalAlignment="Top"
                    Width="Auto" />

                <StackPanel
                    Grid.Column="0"
                    Grid.Row="1"
                    Orientation="Horizontal">
                    <CheckBox Content="CheckBox" Margin="5" />
                    <CheckBox
                        Content="CheckBox"
                        IsEnabled="False"
                        Margin="5" />
                </StackPanel>

                <StackPanel
                    Grid.Column="0"
                    Grid.Row="2"
                    Orientation="Horizontal">
                    <CheckBox
                        Content="CheckBox"
                        IsChecked="True"
                        Margin="5" />
                    <CheckBox
                        Content="CheckBox"
                        IsChecked="True"
                        IsEnabled="False"
                        Margin="5" />
                </StackPanel>

                <StackPanel
                    Grid.Column="0"
                    Grid.Row="3"
                    Orientation="Horizontal">
                    <CheckBox
                        Content="CheckBox"
                        IsChecked="{x:Null}"
                        IsThreeState="True"
                        Margin="5" />
                    <CheckBox
                        Content="CheckBox"
                        IsChecked="{x:Null}"
                        IsEnabled="False"
                        IsThreeState="True"
                        Margin="5" />
                </StackPanel>
            </Grid>
