                        Margin="5"
                        Style="{StaticResource BtnOutline}"
                        wpf:Button.Icon="User" />
                </StackPanel>
                <StackPanel
                    Grid.Column="1"
                    Grid.Row="6"
                    Orientation="Horizontal">
                    <Button
                        Margin="5"
                        Style="{StaticResource BtnOutline}"
                        wpf:Button.ProgressBarMaximum="100"
                        wpf:Button.ProgressBarMinimum="0"
                        wpf:Button.ProgressBarShow="True"
                        wpf:Button.ProgressBarValue="{Binding ElementName=SliderBtn, Path=Value}" />
                    <Button
                        IsEnabled="False"
                        Margin="5"
                        Style="{StaticResource BtnOutline}"
                        wpf:Button.ProgressBarMaximum="100"
                        wpf:Button.ProgressBarMinimum="0"
