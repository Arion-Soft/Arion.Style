                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="60" />
                </Grid.RowDefinitions>

                <Label
                    Content="Icons"
                    FontSize="32"
                    Grid.Column="0"
                    Grid.Row="0"
                    HorizontalAlignment="Left"
                    Margin="10"
                    Padding="0"
                    Style="{StaticResource Lbl}"
                    VerticalAlignment="Top"
                    Width="Auto" />
                <ListView
                    Grid.Column="0"
                    Grid.ColumnSpan="2"
                    Grid.Row="1"
                    Name="LvIcons"
                    ScrollViewer.HorizontalScrollBarVisibility="Disabled">
                    <ListView.ItemsPanel>
                        <ItemsPanelTemplate>
                            <WrapPanel HorizontalAlignment="Center" Orientation="Horizontal" />
                        </ItemsPanelTemplate>
                    </ListView.ItemsPanel>
                    <ListView.ItemTemplate>
                        <DataTemplate DataType="{x:Type controls:Icon}">
                            <Grid Height="100" Width="80">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="5" />
                                    <RowDefinition Height="40" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <controls:Icon
                                    Fill="#252C32"
                                    Grid.Row="1"
                                    Height="40"
                                    Kind="{Binding Kind}"
                                    Width="40" />
                                <TextBlock
                                    FontSize="14"
                                    FontWeight="Light"
                                    Grid.Row="2"
                                    Text="{Binding Kind}"
                                    TextAlignment="Center"
                                    TextWrapping="Wrap" />
                            </Grid>
                        </DataTemplate>
                    </ListView.ItemTemplate>
                </ListView>

                <StackPanel
                    Grid.Column="0"
                    Grid.ColumnSpan="2"
                    Grid.Row="2"
                    Orientation="Horizontal">
                    <TextBox
                        CharacterCasing="Lower"
                        Name="TbFind"
